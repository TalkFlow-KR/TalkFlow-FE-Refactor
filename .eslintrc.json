{
  "extends": ["react-app", "prettier", "airbnb","plugin:react/recommended", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "react/jsx-closing-bracket-location": "warn",
    "semi": ["warn", "always"],
    "prettier/prettier": ["error",
      {"endOfLine": "auto"}],
    "react/jsx-filename-extension": 0
  }
}
